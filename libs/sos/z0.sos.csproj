<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <Import Project="$(ImportDefs)microsoft.extensions.props"/>
  <Import Project="$(ImportDefs)usings.props"/>

  <ItemGroup>
    <ProjectReference Include="$(Containers)" />
    <ProjectReference Include="$(Digital)" />
    <ProjectReference Include="$(Symbolics)" />
    <ProjectReference Include="$(CmdSpecs)" />
    <ProjectReference Include="$(Files)" />
  </ItemGroup>


  <ItemGroup>
    <ContentWithTargetPath Include="$(ZPack)\diagnostics\sos.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <TargetPath>sos.dll</TargetPath>
    </ContentWithTargetPath>

    <ContentWithTargetPath Include="$(ZPack)\diagnostics\sos.pdb">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <TargetPath>sos.pdb</TargetPath>
    </ContentWithTargetPath>
  </ItemGroup>

</Project>
<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <DebugSymbols>true</DebugSymbols>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <Import Project="$(ImportDefs)common.libs.props"/>

  <Import Project="$(ImportDefs)diagnostics.props"/>

  <ItemGroup>
    <Reference Include="$(ZLibName)" HintPath="$(ZLibPath)"/>
    <ProjectReference Include="$(Diagnostics)"/>
  </ItemGroup>

  <ItemGroup>
    <ContentWithTargetPath Include="$(ZPack)\diagnostics\sos.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <TargetPath>sos.dll</TargetPath>
    </ContentWithTargetPath>

    <ContentWithTargetPath Include="$(ZPack)\diagnostics\sos.pdb">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <TargetPath>sos.pdb</TargetPath>
    </ContentWithTargetPath>
  </ItemGroup>

</Project>